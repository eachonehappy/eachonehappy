<div class="col-md-10">
<div class="ibox-title" style="background-color: white;">
      <h1 class="title pull-left" style="margin-top: 10px;
    margin-bottom: 10px; margin-left: 10px;">
        <%= "Jobs" %>
      </h1>
      <div class="pull-right">
      <%= link_to "All Jobs", jobs_path("all"), data: { turbolinks: false, disable_with: 'Wait ...' }, class: 'btn btn-primary', :style=> "margin-top: 10px;
    margin-right: 10px;"%>
    <%= link_to "Completed Jobs", jobs_path("completed"), data: { turbolinks: false,disable_with: 'Wait ...' }, class: 'btn btn-primary', :style=> "margin-top: 10px;
    margin-right: 10px;"%>
    <%= link_to "Unompleted Jobs", jobs_path("uncompleted"), data: { turbolinks: false,disable_with: 'Wait ...' }, class: 'btn btn-primary', :style=> "margin-top: 10px;
    margin-right: 10px;"%>
        <%= link_to "Create a Job", new_job_path, data: { turbolinks: false,disable_with: 'Wait ...' }, class: 'btn btn-primary pull-right ', :style=> "margin-top: 10px;
    margin-right: 10px;"%>
    </div>
      <div class="clearfix"></div>
   
</div>

<div class="panel panel-default">
    <!-- Data table -->
    <table data-toggle="data-table" class="table" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Title</th>
          <th>Description</th>
          <th>Assigned To</th>
          <th>Action</th>
        </tr>
      </thead>
      <tfoot>
        <tr>
          <th>Title</th>
          <th>Description</th>
          <th>Assigned To</th>
          <th>Action</th>
        </tr>
      </tfoot>
      <tbody>
      <%= render @jobs%>
      </tbody>
    </table>
    <!-- // Data table -->
  </div>
  <div style="padding-left:50px;">
   <%= render 'shared/uberad' %>
</div>

</div>

 <div class="col-md-2" style="padding-left: 10px;
    padding-right: 0px;">
    <div class="row">
  <% (1..3).each do |i| %>

    <%= render 'shared/add' %>
  <% end %>
  </div>
    </div>  