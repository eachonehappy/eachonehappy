<div class="col-md-7">
  <div class="panel panel-default">
      <div class="panel-heading panel-heading-gray">Enter Your Details</div>
      <div class="panel-body">
     
          <div class="form-horizontal" role="form">
          <%= form_for(@fundraise_payment_detail, validate: true) do |f| %>
              <div class="form-group">
              <div for="inputEmail3" class="col-sm-3 control-label text-bold"><strong> Full Name</strong></div>
                  <div class="col-sm-9">
                  <%= f.text_field :full_name, class: 'form-control', :placeholder=>"Name" %>
                    
                  </div>
              </div>
              <div class="form-group">
                  <div for="inputEmail3" class="col-sm-3 control-label text-bold"><strong> Account Number</strong></div>
                  <div class="col-sm-9">
                  <%= f.number_field :account_number, class: 'form-control',  :placeholder=>"account number" %>
                    
                  </div>
              </div>
              <div class="form-group">
                  <div for="inputEmail3" class="col-sm-3 control-label text-bold"><strong> IFSC code</strong></div>
                  <div class="col-sm-9">
                  <%= f.text_field :ifsc_code, class: 'form-control', :placeholder=>"ifsc code" %>
                    
                  </div>
              </div>
              
              <div class="form-group">
                  <label class="col-sm-3 control-label">Select Fundraise</label>
                  <div class="col-sm-9">
                 
   
             <%= f.select(:fundraise_id, @all_fundraises.collect {|c| [c.subject, c.id]},{},
                                   { :size => 5,:class=>"selectpicker","data-live-search" =>"true","data-style"=>"btn-white" }
                                 ) %>

                  </div>
              </div>
              
              
              
                     
              <div class="form-group">
                    <div class="col-sm-offset-3 col-sm-9">
                        <%= f.submit "Submit", class: "btn btn-primary" %>
                    </div>
                </div>
              
              <% end %>
          </div>
          
      </div>
  </div>
               
</div>