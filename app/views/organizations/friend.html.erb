<div class="col-md-9" style="padding:0px;">



<div class="panel panel-default">
    <!-- Data table -->
    <table data-toggle="data-table" class="table" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Action</th>
        </tr>
      </thead>
      <tfoot>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Action</th>
        </tr>
      </tfoot>
      <tbody>
      <% @users.each do |user|%>
      <tr>
  <td>
  <% if user.profile_image_url %> 
    <%= image_tag(user.profile_image_url(:thumb)) %>
    <% else %>
    <img src="/images/profile.png" width="35" class="img-circle" />
  <% end %>
  <%= link_to "#{user.first_name}  #{user.last_name}", user_path(user), :class=>"text-capitalize" %></td>
  <td><%= user.email%>
  </td>
  <td>
    <%unless user.organization_users.where(:organization_id => @organization_id).present?%>
    <%= link_to "Invite", organization_invite_path(:user_id => user.id, :organization_id => @organization_id), :method => :post, :class=>"btn btn-primary" %>
    <%else%>
    <a class="btn btn-danger"> Invitation Send</a>
    <%end%>
  </td>
</tr>
      <%end%>
      </tbody>
    </table>
    <!-- // Data table -->
  </div>
  </div>