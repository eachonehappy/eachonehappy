<div class="collapse" id="collapseExampleorganization<%=@organization.id%>">
 <div class="col-md-6" style="padding-left:5px;">
  <%= form_for @organization,  :html =>{:multipart => true} do |f| %>           
    <div class="form-group">
      <label for="inputEmail3" class="col-sm-2 control-label" style="padding-left:5px;">Profile Picture</label> 
      <div class="col-sm-8" style="padding-left:5px;">  
        <%= f.file_field :profile_image, validate: { presence: true } , :class=>"btn btn-default btn-sm pull-left"%>
      </div>
    </div>
    <div class="col-sm-2" style="padding-left:3px;">
      <%= f.submit "Change", :class=> "btn btn-primary" %>
    </div>
  <%end%>
</div> 
 <div class="col-md-6" style="padding-left:5px;">
  <%= form_for @organization, :html =>{:multipart => true} do |f| %>           
    <div class="form-group" > 
      <label for="inputEmail3" class="col-sm-2 control-label">Cover Photo</label> 
      <div class="col-sm-8" style="padding-left:5px;">          
        <%= f.file_field :cover_image , :class=>"btn btn-default btn-sm pull-left"%>
      </div>
    </div>
    <div class="col-sm-2" style="padding-left:2px;">
      <%= f.submit "Change", :class=> "btn btn-primary" %>
    </div>
  <%end%>
</div>       
 
</div> 