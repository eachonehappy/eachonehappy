<div class="col-md-9">
	<div class="ibox-title" style="background-color: white;">
    <h1 class="title pull-left" style="margin-top: 10px;
  margin-bottom: 10px; margin-left: 10px;">
      <%= "Organizations" %>
    </h1>
     <form class="navbar-form navbar-left hidden-xs" role="search">
      <%= form_tag organizations_path, :method => 'get' do%>
        <div class="search-2">
          <div class="input-group">
           
            <%= text_field_tag :search, params[:search],:class=>"form-control form-control-w-150", :placeholder=>"Search .."  %>
            
            <span class="input-group-btn">
            <%= submit_tag 'Search', :class=>"btn btn-primary"%>
             
            </span>
          </div>
        </div>
        <%end%>
      </form>
    <%= link_to "Start a Organization", new_organization_path, data: { turbolinks: false,disable_with: 'Wait ...' }, class: 'btn btn-primary pull-right ', :style=> "margin-top: 10px;
	    margin-right: 10px;"%>
	  <div class="clearfix"></div>   
	</div>
	<div class="timeline row" data-toggle="gridalicious" style="padding-top: 10px;">
	 <%= render @organizations %>               
	</div>
</div>